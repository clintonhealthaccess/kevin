<?xml version="1.0" encoding="UTF-8"?>

<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd" >

    <diskStore path="java.io.tmpdir"/>

    <cacheManagerEventListenerFactory class="" properties=""/>

    <defaultCache
            maxElementsInMemory="5000"
            eternal="false"
            timeToLiveSeconds="3600"
            overflowToDisk="false"
            diskPersistent="false"
    />

    <cache name="org.chai.kevin.dashboard.DashboardPercentage"
           maxElementsInMemory="50000"
           timeToIdleSeconds="300"
    />

    <cache name="org.hibernate.cache.UpdateTimestampsCache"
           maxElementsInMemory="10000"
           timeToIdleSeconds="300"
    />

    <cache name="org.hibernate.cache.StandardQueryCache"
           maxElementsInMemory="10000"
           timeToIdleSeconds="300"
    />
    
    <cache name="org.hisp.dhis.organisationunit.OrganisationUnit.children"
           maxElementsInMemory="5000"
           timeToIdleSeconds="300"
    />
    
    <cache name="org.hisp.dhis.period.Period"
           maxElementsInMemory="10"
           timeToIdleSeconds="300"
    />
    
    <cache name="org.chai.kevin.DataElement"
           maxElementsInMemory="5000"
           timeToIdleSeconds="300"
    />
    
    <cache name="org.chai.kevin.Expression"
           maxElementsInMemory="5000"
           timeToIdleSeconds="300"
    />
    
    
    <cache name="org.hibernate.cache.dashboard.DashboardPercentage"
           maxElementsInMemory="500000"
           timeToIdleSeconds="300"    	
    />
    
    <cache name="org.hisp.dhis.organisationunit.OrganisationUnit"
           maxElementsInMemory="5000"
           timeToIdleSeconds="300"
    />
    
    <cache name="org.chai.kevin.DataValue"
           maxElementsInMemory="1000000"
           timeToIdleSeconds="300"
    />
    
    <!-- not sure it is used -->
    <cache name="org.hibernate.cache.DataValueQueryCache"
           maxElementsInMemory="1000000"
           timeToIdleSeconds="300"
    />
    
	<cache name="org.hibernate.cache.ExpressionValueQueryCache"
           maxElementsInMemory="1000000"
           timeToIdleSeconds="300"
    />
    
	<cache name="org.hibernate.cache.CalculationValueQueryCache"
           maxElementsInMemory="1000000"
           timeToIdleSeconds="300"
    />
    
    <!-- not sure it is used -->
    <cache name="org.hibernate.cache.DashboardPercentageQueryCache"
           maxElementsInMemory="100000"
           timeToIdleSeconds="300"
    />


</ehcache>